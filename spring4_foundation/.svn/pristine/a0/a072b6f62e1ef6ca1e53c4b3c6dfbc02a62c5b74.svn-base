package com.systran.thread;

public class ThreadTesting extends Thread{
	public void run()
	{
		
		try {
			for(int i=0;i<10;i++)
			{
				System.out.println(i);
				Thread.sleep(1*1000);
			}
		}
		catch (InterruptedException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
	}
}
